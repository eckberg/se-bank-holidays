    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.3",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
